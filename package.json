{
  "private": true,
  "name": "hmmmm",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "react/jsx-indent-props": [
        2,
        2
      ]
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-synacor": "^3.0.4",
    "if-env": "^1.0.0",
    "preact-cli": "^3.0.0-rc.6"
  },
  "dependencies": {
    "goober": "^1.6.1",
    "mobx": "^5.15.1",
    "mobx-react-lite": "^1.5.2",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2"
  }
}
